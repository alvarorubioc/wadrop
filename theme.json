{
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json",
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/button": {
				"border": {
					"radius": true
				}
			},
			"core/group": {
				"color": {
					"background": true
				}
			}
		},
		"border": {
			"color": true,
			"style": true,
			"width": true
		},
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [],
			"link": false,
			"palette": [
				{
					"slug": "background",
					"color": "#FFFFFF",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#035072",
					"name": "Text main"
				},
				{
					"slug": "text-medium",
					"color": "var(--wp--custom--color--neutral-60)",
					"name": "Text medium"
				},
				{
					"slug": "primary-10",
					"color": "#F2F7FF",
					"name": "Primary 10"
				},
				{
					"slug": "primary-30",
					"color": "#A9E1FA",
					"name": "Primary 30"
				},
				{
					"slug": "primary",
					"color": "#60CAF4",
					"name": "Primary"
				},
				{
					"slug": "primary-80",
					"color": "#035072",
					"name": "Primary 80"
				},
				{
					"slug": "primary-100",
					"color": "#032C3A",
					"name": "Primary 100"
				},
				{
					"slug": "neutral-10",
					"color": "#F8FAFC",
					"name": "Neutral 10"
					
				},
				{
					"slug": "neutral-30",
					"color": "#CBD5E1",
					"name": "Neutral 30"
					
				},
				{
					"slug": "neutral-60",
					"color": "#475569",
					"name": "Neutral 60"
					
				},
				{
					"slug": "neutral-80",
					"color": "#1E293B",
					"name": "Neutral 80"
					
				},
				{
					"slug": "neutral-100",
					"color": "#020617",
					"name": "Neutral 100"
					
				}

			]
		},
		"custom": {
			"border-radius": {
				"lg": "16px",
				"md": "8px",
				"rounded": "999px",
				"sm": "4px",
				"xl": "32px",
				"zero": "0"
			},
			"border-width": {
				"large": "4px",
				"medium": "3px",
				"small": "2px",
				"tiny": "1px"
			},
			"box-shadow": {
				"1": "0px 2px 8px rgba(33, 33, 33, 0.12)",
				"2": "3px 3px 10px 10px rgba(0, 0, 0, 0.10)"
			},
			"color": {
				"link": "var(--wp--preset--color--primary)",
				"neutral-black": "#000000",
				"neutral-white": "#FFFFFF"
			},
			"layout": {
				"block-gap": "2rem",
				"block-gap-large": "40px",
				"content": "1216px",
				"padding": "16px",
				"page": "var(--wp--custom--layout--content)",
				"sidebar": "336px",
				"wide": "90%"
			},
			"letter-spacing": {
				"loose": ".05em",
				"looser": ".1em",
				"none": "normal",
				"tight": "-.01em"
			},
			"line-height": {
				"headings": 1.2,
				"normal": 1.5,
				"tiny": 1
			}
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Space 1 (1rem - 16px)",
					"size": "1rem",
					"slug": "10"
				},
				{
					"name": "Space 2 (1.5rem - 24px)",
					"size": "1.5rem",
					"slug": "20"
				},
				{
					"name": "Space 3 (2rem - 32px)",
					"size": "2rem",
					"slug": "30"
				},
				{
					"name": "Space 4 (2.5rem - 40px)",
					"size": "2.5rem",
					"slug": "40"
				},
				{
					"name": "Space 6 (4rem - 64px)",
					"size": "4rem",
					"slug": "60"
				},
				{
					"name": "Space 7 (4.5rem - 72px)",
					"size": "4.5rem",
					"slug": "70"
				},
				{
					"name": "Space 8 (5rem - 80px)",
					"size": "5rem",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontFamilies": [
				{	
					"name": "Font Main",
					"slug": "font-main",
					"fontFamily": "Manrope, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/Manrope-VariableFont_wght.woff2"
							],
							"fontWeight": "300 500 700",
							"fontStyle": "normal",
							"fontFamily": "Manrope"
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Display",
					"size": "clamp(3rem, 8.8vw, 6.25rem)",
					"slug": "display"
				},
				{
					"name": "H1",
					"size": "clamp(2.25rem, 4.8vw, 2.5rem)",
					"slug": "heading-1"
				},
				{
					"name": "H2",
					"size": "clamp(1.5rem, 4.4vw, 1.75rem)",
					"slug": "heading-2"
				},
				{
					"name": "H3",
					"size": "1.5rem",
					"slug": "heading-3"
				},
				{
					"name": "H4",
					"size": "1.375rem",
					"slug": "heading-4"
				},
				{
					"name": "H5",
					"size": "1.25rem",
					"slug": "heading-5"
				},
				{
					"name": "H6",
					"size": "1.125rem",
					"slug": "heading-6"
				},
				{
					"name": "Subtitle",
					"size": "1.125rem",
					"slug": "subtitle"
				},
				{
					"name": "Body",
					"size": "1rem",
					"slug": "body"
				},
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"name": "Caption",
					"size": "0.75rem",
					"slug": "caption"
				}
			],
			"fontWeight": true,
			"letterSpacing": false,
			"lineHeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/group": {
				"spacing": []
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--rounded)",
					"width": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--primary-100)"
				},
				"spacing": {
					"padding": "calc(1rem + 0px) 2rem"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)",
					"fontSize": "var(--wp--preset--font-size--body)",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)",
					"fontWeight": "500"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-6)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)",
					"lineHeight": "var(--wp--custom--line-height--headings)",
					"fontWeight": "300"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--block-gap)",
			"padding": {
				"bottom": "0px",
				"left": "2rem",
				"right": "2rem",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--font-main)",
			"fontSize": "var(--wp--preset--font-size--body)",
			"lineHeight": "var(--wp--custom--line-height--normal)"
		}
	},
	"version": 3
}